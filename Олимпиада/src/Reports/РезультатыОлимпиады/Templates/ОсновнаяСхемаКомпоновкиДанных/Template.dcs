	</calculatedField>
	<calculatedField>
		<dataPath>ДанКорректныйОтвет</dataPath>
		<expression>ЗначениеЗаполнено(Ответ) И Ответ = КорректныйОтвет</expression>
		<title xsi:type="v8:LocalStringType">
			<v8:item>
				<v8:lang>ru</v8:lang>
